{
  "title": "Qwen Background Remover",
  "description": "Serverless image background remover using Qwen-Image-Lightning LoRA",
  "type": "serverless",
  "category": "image",
  "iconUrl": "https://huggingface.co/lightx2v/Qwen-Image-Lightning/resolve/main/thumbnail.png",
  "model": "Qwen/Qwen-Image",
  "config": {
    "runsOn": "GPU",
    "gpuCount": 1,
    "containerDiskInGb": 20,
    "presets": [
      {
        "name": "T4 Default",
        "defaults": {
          "MODEL_PATH": "Qwen/Qwen-Image",
          "LORA_PATH": "lightx2v/Qwen-Image-Lightning",
          "LORA_WEIGHT": "Qwen-Image-Lightning-8steps-V1.0.safetensors"
        }
      }
    ]
  }
}
